<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Orangutan SVP-1284</Name>
    <Description>Simple template for the Pololu Orangutan SVP-1284, using the Pololu AVR C/C++ Library.</Description>
    <ProjectType>AVRGCC</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>20</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>orangutan_app</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>svp_icon.jpg</Icon>
    <PreviewImage>svp_preview.jpg</PreviewImage>
  </TemplateData>
  <TemplateContent>
    <Project File="template.cproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" OpenInEditor="true">main.c</ProjectItem>
    </Project>
    <CustomParameters>
      <CustomParameter Name="$mcu$" Value="ATmega1284P"/>
      <CustomParameter Name="$pololu_lib$" Value="pololu_atmega1284p"/>
      <CustomParameter Name="$boardmacro$" Value=""/>
    </CustomParameters>
  </TemplateContent>
</VSTemplate>
